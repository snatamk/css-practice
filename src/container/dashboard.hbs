<html>

  <head>
    <title>
      Dashboard page
    </title>

    <meta charset='utf-8' />

    <link rel='apple-touch-icon' sizes='180x180' href='/fav/apple-touch-icon.png' />
    <link rel='icon' type='image/png' sizes='32x32' href='/fav/favicon-32x32.png' />
    <link rel='icon' type='image/png' sizes='16x16' href='/fav/favicon-16x16.png' />
    <link rel='manifest' href='fav/site.webmanifest' />
    <link rel='mask-icon' href='/fav/safari-pinned-tab.svg' color='#5bbad5' />
    <meta name='msapplication-TileColor' content='#da532c' />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width,
     initial-scale=1.0, nminimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />

  </head>

  <body>
    <div class='page page--dashboard'>
      <aside class='page__aside aside vertical'>
        <div class='text aside__logo'>Sidebar</div>

        <nav class='vertical vertical--small'>
          <a href='#' class='nav-button nav-button--active click'>
            <span class='icon nav-button__icon' style="background-image: url('/svg/nav/home.svg')"></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='#' class='nav-button click'>
            <span class='icon nav-button__icon' style="background-image: url('/svg/nav/orders.svg')"></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='#' class='nav-button click'>
            <span class='icon nav-button__icon' style="background-image: url('/svg/nav/products.svg')"></span>
            <span class='text text--light'>Products</span>
          </a>
        </nav>
      </aside>
      <header class='header'>
        <div class='layout header__content text--small'>
          <div class='header__search relative'>
            <input type='text' placeholder='Search' class='text field field--header' />
            <span class='icon header__icon--search icon--size_small' style="background-image: url('/svg/header/search.svg')"></span>
            <span
              class='icon icon--header--settings icon--button icon-color_blue click'
              style="background-image: url('/svg/header/settings.svg')"
            ></span>
          </div>
          <div class='header__4408 relative'>
            <span
              class='icon icon--zise_big icon-color_yellow icon--button click'
              style="background-image: url('/svg/header/antenna.svg')"
            ></span>
            <span
              class='icon icon--zise_big icon-color_sky icon--button click'
              style="background-image: url('/svg/header/symbols.svg')"
            ></span>
            <span
              class='icon icon--zise_big icon-color_yellow icon--button click'
              style="background-image: url('/svg/header/bell.svg')"
            ></span>
          </div>
        </div>
      </header>
      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>
            <div class='analytic analytic--double analytic--purple'>
              <span
                class='icon analytic__button icon--button icon-color_purple click'
                style="background-image: url('/svg/analytic/money.svg')"
              ></span>
              <div class='relative vertical vertical--small'>
                <div class='analytic__title text text--light text--thick'>$500.00</div>
                <div class='text text--small text--line text--gray_ligt text--medium'>Total earning</div>

                <span
                  class='icon analytic__arrow icon--size_small click'
                  style="background-image: url('/svg/analytic/blue-arrow.svg')"
                ></span>
              </div>
            </div>

            <div class='analytic analytic--double analytic--blue'>
              <span
                class='icon analytic__button icon--button icon-color_blue click'
                style="background-image: url('/svg/analytic/shopping.svg')"
              ></span>
              <div class='relative vertical vertical--small'>
                <div class='analytic__title text text--light text--thick'>$961</div>
                <div class='text text--small text--line text--gray_ligt text--medium'>Total Order</div>
              </div>
            </div>

            <div class='analytic analytic__content analytic--blue'>
              <span
                class='icon analytic__button icon--button icon-color_blue click'
                style="background-image: url('/svg/analytic/money.svg')"
              ></span>
              <div class='relative vertical vertical--small'>
                <div class='analytic__title text text--light text--thick'>$203k</div>
                <div class='text text--small text--line text--gray_ligt text--medium'>Total Income</div>
              </div>
            </div>

            <div class='analytic analytic__content analytic--border'>
              <span
                class='icon analytic__button icon--button icon-color_yellow click'
                style="background-image: url('/svg/analytic/shop.svg')"
              ></span>
              <div class='relative vertical vertical--small'>
                <div class='analytic__title text text--thick'>$203k</div>
                <div class='text text--small text--line text--gray text--medium'>Total Income</div>
              </div>
            </div>
          </section>

          <section class='list-payment'>
            <div class='box'>
              <p class='text list__title text--line text--thick'> Purchase list</p>
              <hr class='divider' />

              <div class='list vertical--small'>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>ID437661</span>
                  <span class='text text--line text--medium'>May 11, 2023, 14:21</span>
                </div>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>AirOn ProCa</span>
                  <span class='text text--line text--medium'>7/8 USB Type-C</span>
                </div>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>132.32 EUR</span>
                  <span class='text text--line text--medium text--red'>Not paid</span>
                </div>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>Customer</span>
                  <span class='text text--line text--medium'>robin@com.de</span>
                </div>
              </div>
              <hr class='divider' />
              <div class='list vertical--small'>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>ID437661</span>
                  <span class='text text--line text--medium'>May 11, 2023, 14:21</span>
                </div>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>PHILIPS Essential</span>
                  <span class='text text--line text--medium'>HD9200/90</span>
                </div>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>543 USD</span>
                  <span class='text text--line text--medium text--green'>Pay by Stripe</span>
                </div>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>Customer</span>
                  <span class='text text--line text--medium'>dima@com.ua</span>
                </div>
              </div>
            </div>

            <div class='box'>
              <p class='text list__title divider text--line text--thick'> Payment</p>
              <div class='payment-checkbox'>
                <input type='checkbox' name='checkbox' id='checkbox' />
                <div></div>
                <label for='checkbox' class='text text--line text--medium'>Credit card</label>
              </div>

              <div class='payment-checkbox'>
                <input type='checkbox' name='checkbox' id='checkbox' />
                <div></div>
                <label for='checkbox' class='text text--line text--medium'>Debit card</label>
              </div>

              <div class='payment-checkbox'>
                <input type='checkbox' name='checkbox' id='checkbox' />
                <div></div>
                <label for='checkbox' class='text text--line text--medium'>PayPal</label>
              </div>

              <div class='payment-two divider'>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>Name on card</span>
                  <input type='text' class='text field' />
                </div>
                <div class='list__span vertical--small'>
                  <sapn class='text text--line text--medium'>Credit card number</sapn>
                  <input type='text' class='text field' />
                </div>

                <div class='list__span vertical--small'>
                  <sapn class='text text--line text--medium'>Expiration</sapn>
                  <input type='text' class='text field' />
                </div>

                <div class='list__span vertical--small'>
                  <sapn class='text text--line text--medium'>CVV</sapn>
                  <input type='text' class='text field' />
                </div>
              </div>

              <div class='payment-two divider'>
                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>Country</span>
                  <div class='relative click'>
                    <div class='field field--select relative'>
                      <input type='text' placeholder='Choose...' class='text text--light field--select' />
                      <select class='relative__mark'></select>

                      <select class='field__select'>
                        <option value='ukraine'>Ukraine</option>
                        <option value='poland'>Poland</option>
                        <option value='great britan'>Great Britain</option>
                        <option value='usa'>USA</option>
                      </select>
                    </div>
                  </div>
                </div>

                <div class='list__span vertical--small'>
                  <span class='text text--line text--medium'>State</span>
                  <div class='relative click'>
                    <div class='field field--select relative'>

                      <input type='text' placeholder='Choose...' class='text text--light field--select' />
                      <select class='relative__mark'></select>
                    </div>
                    <select class='field__select'>
                      <option value='1'>1</option>
                      <option value='2'>2</option>
                      <option value='3'>3</option>
                      <option value='4'>4</option>
                    </select>
                  </div>
                </div>
              </div>

              <div class='payment-button'>
                <a href='#' class='nav-button nav-button--active click'>
                  <span class='text payment__button text--light'>Continue to checkout</span>
                </a>
              </div>

            </div>
          </section>
        </div>
      </main>
    </div>
  </body>
</html>